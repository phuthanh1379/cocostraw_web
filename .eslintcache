arams.name;\n        this.product = products[id-1];\n\n\n        return (\n            <React.Fragment>\n                {/*{this.state.showModal ? <Modal handleEventButton={this.showModal}/> : \"\"}*/}\n                <div className=\"product\">\n                    <div className=\"product-detail-container\" style={{backgroundImage: `url(${this.product.image[0]})`}}>\n                        <div className=\"product-detail-description\">\n                            <div className=\"product-category\">\n                                {this.product.category}\n                            </div>\n                            <div className=\"product-name\">\n                                {this.product.name}\n                            </div>\n                            <div className=\"product-price\">\n                                {this.product.price}.000đ\n                            </div>\n                            <div className=\"product-description\">\n                                {this.product.description}\n                            </div>\n                            <div className=\"product-detail-title\">\n                                <div onClick={this.handleProductDetailClick}>\n                                    CHI TIẾT SẢN PHẨM\n                                    {this.state.productDetailClicked ?\n                                        <IoMdArrowDropdown/> : <IoMdArrowDropright/>\n                                    }\n                                </div>\n                            </div>\n                            {\n                                this.state.productDetailClicked ? this.showProductDetail(this.product.detail) : \"\"\n                            }\n                            <div className=\"product-detail-title\">\n                                <div onClick={this.handleShippingPolicyClick}>\n                                    CHÍNH SÁCH VẬN CHUYỂN VÀ ĐỔI TRẢ\n                                    {this.state.shippingPolicyClicked ?\n                                        <IoMdArrowDropdown/> : <IoMdArrowDropright/>\n                                    }\n                                </div>\n                            </div>\n                            {\n                                this.state.shippingPolicyClicked ? this.showShippingPolicy(this.product.shippingPolicy) : \"\"\n                            }\n                            {\n                                this.state.shippingPolicyClicked ? this.showShippingPolicyNote(this.product.shippingNote) : \"\"\n                            }\n                        </div>\n                    </div>\n                    {/*<div className=\"product-purchase\">*/}\n                    {/*    <OutlineButton handleEventButton={this.showModal}>ĐẶT HÀNG</OutlineButton>*/}\n                    {/*</div>*/}\n                    <div>\n                        <Testimonial img={this.product.image[1]} title={testimonial.title1} text={testimonial.text1}/>\n                        <Testimonial backward={true} img={this.product.image[2]} title={testimonial.title2} text={testimonial.text2}/>\n                    </div>\n                    <div>\n                        <img src={this.product.image[3]} alt=\"hình\"/>\n                    </div>\n                    <div className=\"product-others\">\n                        XEM THỬ CÁC SẢN PHẨM KHÁC NHÉ\n                    </div>\n                    <div className=\"product-slider\">\n                        <MultipleSlider {...this.props}/>\n                    </div>\n                    <div className=\"back-to-home\">\n                        <button onClick={() => this.props.history.push(\"/\")}>\n                            VỀ TRANG CHỦ\n                        </button>\n                    </div>\n                    <div className=\"back-to-top\" onClick={() => scroll.scrollToTop()}>\n                        <IoMdArrowDropup/> BACK TO TOP\n                    </div>\n                </div>\n            </React.Fragment>\n        );\n    }\n}\n\nexport default Product;\n","E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\components\\button\\OutlineButton.jsx",[],"E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\components\\slider\\MultipleSlider.jsx",["107"],"import React, {useState} from 'react';\nimport {SliderData} from \"../../resources/SlideData\";\nimport './MultipleSlider.scss'\n\nconst MultipleSlider = (props) => {\n\n    return (\n        <div className=\"slider\">\n            {SliderData.map((slide, index) => {\n                return (\n                    <div key={index} className=\"slider-container\">\n                        <div className=\"multiple-slider-image\">\n                            <img src={slide.image}\n                            alt={index}\n                            onClick={\n                                () => props.history.push(slide.path)}/>\n                        </div>\n                        <div className=\"multiple-slider-name\">{slide.name}</div>\n                    </div>\n                );\n            })}\n        </div>\n    );\n};\n\nexport default MultipleSlider;\n","E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\components\\testimonial\\Testimonial.jsx",[],"E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\components\\modal\\Modal.jsx",[],"E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\resources\\SlideData.js",[],"E:\\Personal\\CoCoStraw\\cocostraw_web\\src\\components\\scrollToTop\\ScrollToTop.jsx",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":1,"column":16,"nodeType":"114","messageId":"115","endLine":1,"endColumn":22},{"ruleId":"112","severity":1,"message":"116","line":1,"column":24,"nodeType":"114","messageId":"115","endLine":1,"endColumn":39},{"ruleId":"112","severity":1,"message":"117","line":7,"column":8,"nodeType":"114","messageId":"115","endLine":7,"endColumn":14},{"ruleId":"112","severity